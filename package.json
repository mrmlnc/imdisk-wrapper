{
  "name": "imdisk-wrapper",
  "version": "1.0.1",
  "description": "A simple wrapper for ImDisk to create (only VM) and delete virtual disks",
  "license": "MIT",
  "repository": "mrmlnc/imdisk-wrapper",
  "author": {
    "name": "Denis Malinochkin",
    "url": "canonium.com"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "os": [
    "win32"
  ],
  "scripts": {
    "test": "xo && node test.js"
  },
  "keywords": [
    "windows",
    "ram",
    "ramdisk",
    "imdisk",
    "virtual"
  ],
  "files": [
    "README.md",
    "index.js"
  ],
  "dependencies": {
    "windows-drive-letters": "^2.0.0"
  },
  "devDependencies": {
    "ava": "^0.2.0",
    "xo": "^0.9.0"
  },
  "xo": {
    "space": true,
    "rules": {
      "object-curly-spacing": [
        2,
        "always"
      ],
      "space-before-function-paren": 0
    },
    "envs": [
      "node"
    ]
  }
}
